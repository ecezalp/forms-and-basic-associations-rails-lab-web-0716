<%= form_for @song  do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :artist_name %>
<%= f.text_field :artist_name, list: "artist_name_autocomplete" %>
  <datalist id="artist_name_autocomplete">
  <%= Artist.all.each do |artist| %>
  <option value="<%= artist.name %>">
    <% end %>
  </datalist>
 
<%= f.label :genre_name %>
<%= f.collection_select :genre_id, Genre.all, :id, :name %>

<%= f.label :note_contents %>
 <input name='song[note_contents][]' id='song_notes_1'>
<input name='song[note_contents][]' id='song_notes_2'>

<%= f.submit %>
<% end %>
